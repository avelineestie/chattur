{
  "name": "chattur",
  "description": "A real time chat application with bots using React.js and socket.io",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "express": "^4.13.3",
    "jquery": "^2.1.4",
    "socket.io": "^1.3.7",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6"
  },
  "scripts":{
    "parseJSX": "browserify ./public/js/src/app.js -t babelify --outfile ./public/js/dist/app.js",
